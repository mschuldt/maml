arduino = Arduino(desktop=True)

@block(once)
def test():
    # True assignments
    print(1+1 == 2) # 1
    print(1+1 != 0) # 1
    print(1+1 <= 2) # 1
    print(1+1 <= 3) # 1
    print(1+1 < 3)  # 1
    print(1+1 >= 2) # 1
    print(1+1 > 1)  # 1
    # False assignments
    print(1+1 != 2) # 0
    print(1+1 == 0) # 0
    print(1+1 >= 3) # 0
    print(1+1 > 3)  # 0
    print(1+1 <= 1) # 0
    print(1+1 < 1)  # 0
    print("s" == "s")
    # Arithmetic equality tests
    print(1 - 1) # 0
    print(2 - 1) # 1
    print(1 - 2) # -1
    print(4 * 6) # 24
    print(6 * 4) # 24
    print(4 * 0) # 0
    print(0 * 4) # 0
    print(6 / 3) # 2
    print(6 / 4) # 1
    print(0 / 4) # 0

    print(-1 + 2) # 1
    die()

arduino.send(test)
